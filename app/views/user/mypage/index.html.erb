<%= form_with model: @youte, url:user_youtes_path do |f| %>
      <label >Youte!</label>
      <%= f.text_field :text %>
      <label>ジャンル</label>
      <% f.text_field :genre_id, class:"form-control" %>
      <%= f.select :genre_id, options_from_collection_for_select(YouteGenre.all, :id, :name_was), include_blank: "--選択してください--" %>

     <label class="col-4 mx-auto d-block"><%= f.submit 'Youte!!', class:"btn btn-success" %></label>

  <% end %>