<div class="container">
  <h2>Youteジャンル編集</h2>
  <div class="row">
    <div class="col-10 mt-5 mx-auto">
      <table>
        <td>Youteジャンル名<td>
        <td>
          <%= form_with model:  @youte_genre, url: admin_genre_youte_path do |f| %>
            <%= f.text_field :genre_name %>
            <input type="submit" name="commit" value="変更を保存" class="btn btn-success" data-disable-with="Update Genre">
          <% end %>
        </td>
      </table>
    </div>
  </div>
</div>